@import "../_theme.styl";
    
.footer-bg 
    background-color: footer-bg;
    footer 
        height: 100px;
        font-size: 14px;
        color: #717171;
        margin: 0 !important;
        a
            color: black;

#back-to-top 
    background-color: white;
    z-index: 1;
    cursor: pointer;
    position: fixed;
    height: 2.6em;
    width: 2.7em;
    border-radius: 12px;
    border: 1px solid separator-color;
    transition: 0.2 ease-out;
    -webkit-transition: 0.2s ease-out;
    bottom: 100px;
    right: 10%;
    display: none;
    img
        margin: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
        overflow: hidden;
        filter: invert(33%) sepia(0%) saturate(0%) hue-rotate(203deg) brightness(95%) contrast(93%);